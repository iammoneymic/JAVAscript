// Part 2: Prime Time
function isPrime(num) {
    for (let i = 2; i < num; i++) {
        if (num % i === 0) {
            return false;
        }
    }
    return num > 1;
}

function findNextPrime(n) {
    while (true) {
        if (isPrime(n)) {
            console.log(n);
            break;
        }
        n++;
    }
}

findNextPrime(9);





